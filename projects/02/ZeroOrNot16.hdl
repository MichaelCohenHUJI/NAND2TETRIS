
CHIP ZeroOrNot16 {
    IN
        x[16],
        zx, // zero the x input?
        nx;

    OUT
        out[16]; // 16-bit output

    PARTS:
        //Zero16(in=x, out=zeros );
        Mux16(a=x,b[0..15]=false,sel=zx,out=out1);
        Not16(in=out1,out=notx);
        Mux16(a=out1,b=notx,sel=nx,out=out);
}
